  "scripts": {
    "build": "npm run prisma:generate && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "seed:run": "ts-node -r tsconfig-paths/register ./libs/common/src/core/database/seeds/run.seed.ts",
    "prisma:generate": "npx prisma generate --schema prisma/schema.prisma",
    "start:dev": "npm run prisma:generate && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
